# RTSP source pulled by mediaMTX
MEDIA_RPI_RTSP_URL=rtsp://example.local:8554/stream

# FastAPI endpoint that receives events (include /events path)
FASTAPI_ENDPOINT=http://localhost:8000/events
FASTAPI_TOKEN=local-dev-token

# Model and artifact paths
YOLO_MODEL_PATH=models/yolov8n-custom.pt
MEDIA_OUTPUT_ROOT=./artifacts

# Runtime flags
GPU_ENABLED=false
STREAM_DEFAULT_FPS=12
